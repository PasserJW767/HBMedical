<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//MYBATIS.ORG//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanqiao.zzy.mapper.HomePageCenterDevelopMapper">

    <select id="calCountByOrgidentify" parameterType="string" resultType="int">
        select count(*)
        from organization
        where ORG_IDENTIFY=#{orgidentify}
    </select>
</mapper>